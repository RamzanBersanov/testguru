<tr>
  <td><%= test.title %></td><br>
  <td><%= test_level(test) %></td><br>
  <td><%= test.questions.count %></td><br>
  <td><%= link_to t('.show_test'), admin_test_path(test) %></td><br>
  <td><%= link_to t('.edit_test'), edit_admin_test_path(test) %></td><br>
  <td>
    <%= link_to t('.delete_test'),
                  admin_test_path(test),
                  method: :delete,
                  data: { confirm: t('.are_you_sure') } %>
  </td><br>
</tr>
