<tr>
  <td><%= test.title %></td><br>
  <td><%= test_level(test) %></td><br>
  <td><%= test.questions.count %></td><br>
  <td><%= link_to 'Показать тест', admin_test_path(test) %></td><br>
  <td><%= link_to 'Редактировать тест', edit_admin_test_path(test) %></td><br>
  <td>
    <%= link_to 'Удалить тест',
                  admin_test_path(test),
                  method: :delete,
                  data: { confirm: 'Are you sure?' } %>
  </td><br>
</tr>
