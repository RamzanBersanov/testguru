<%= form_with model: [:admin, @test, @question], url: admin_test_questions_path(@test, @question) do |form| %>
  <%= render 'shared/errors', resource: @test %>
  <%= form.text_field :body %>

  <%= form.submit %>

  <% form.label :test_id %>
  <% form.hidden_field :test_id, value: @test.id %>

<% end %>

<%= render 'back' %>
